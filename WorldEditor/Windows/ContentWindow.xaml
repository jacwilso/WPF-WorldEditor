<Window x:Class="WorldEditor.Windows.ContentWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WorldEditor.Windows"
        mc:Ignorable="d"
        Title="WindowWindow" Height="450" Width="800">
    <DockPanel>
        <ContentControl Name="LeftDock" DockPanel.Dock="Left" />
        <Frame Name="RightDock" DockPanel.Dock="Right"/>
        <ContentControl Name="BottomDock" DockPanel.Dock="Bottom"/>
        <ContentControl Name="CenterDock"/>
        <!--Content Area-->
        <!--<ContentControl Content="{Binding CurrentPageViewModel}" />-->
    </DockPanel>
</Window>
